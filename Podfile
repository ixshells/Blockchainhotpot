    pod 'Alamofire', '~> 4.7'
    pod 'SVProgressHUD', :git => 'https://github.com/SVProgressHUD/SVProgressHUD.git'
    pod 'MJRefresh'
end

target 'BlockchainHotpot' do
    general_pods
end

